<p id="notice"><%= notice %></p>
<h1>Tweets</h1>
<%= link_to 'New Tweet', new_tweet_path %>
    <% @tweets.each do |tweet| %>
      <hr>
      <div>
        <%= tweet.content %>
      </div>
      <div id="posts">
        <%= link_to 'Show', tweet %>
        <%= link_to 'Edit', edit_tweet_path(tweet), data: { confirm: 'Are you sure to edit this tweetï¼Ÿ' } %>
        <%= link_to 'Delete', tweet, method: :delete, data: { confirm: 'Are you sure to delete this tweet?' } %>
      </div>
    <% end %>
<br>
